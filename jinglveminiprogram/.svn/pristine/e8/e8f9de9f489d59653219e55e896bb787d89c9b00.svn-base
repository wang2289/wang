.result-bg {
  width: 100%;
  height: 276rpx;
  background: rgba(255, 0, 156, 0.3);
}

.result-img {
  width: 100%;
  height: 256rpx;
}

.result-r {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 39rpx;
}

.result-box {
  width: 666rpx;
  height: 327rpx;
  background: #fff;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 0px 10rpx 0px rgba(0, 0, 0, 0.11);
  border-radius: 10rpx;
  margin-top: -186rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}

.result-titimg {
  width: 540rpx;
  height: 110rpx;
  margin-top: -55rpx;
}

.result-tit {
  color: #fff;
  margin-top: -94rpx;
  font-size: 32rpx;
}

.time {
  width: 184rpx;
  height: 49rpx;
  background: rgba(247, 247, 249, 1);
  border-radius: 0px 25rpx 25rpx 0px;
  position: absolute;
  top: 87rpx;
  left: 0rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.time image {
  width: 26rpx;
  height: 26rpx;
  margin-left: 13rpx;
}

.timeji {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  margin-left: 6rpx;
}

.card-box {
  display: flex;
  flex-direction: column;
}

.card-title {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 29rpx 30rpx 24rpx 30rpx;
  background: rgba(247, 247, 249, 1);
}

.card-name {
  font-size: 28rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.card-num {
  font-size: 26rpx;
  color: rgba(102, 102, 102, 1);
}

.card-body {
  background: #fff;
  padding-left: 1.5rpx;
  padding-bottom: 41rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.card-list {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

answer-cmp {
  margin-top: 33rpx;
  margin-left: 27.5rpx;
  margin-right: 27.5rpx;
}

.fen {
  margin-top: 88rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.score {
  color: #000;
  font-size: 80rpx;
  font-family: PingFang SC;
  font-weight: bold;
  line-height: 1;
}

.zongscore {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.error {
  background: rgba(255, 240, 240, 1);
  border: 1px solid rgba(254, 63, 54, 1);
  color: #fe3f36;
}

.right {
  background: rgba(249, 255, 235, 1);
  border: 1px solid rgba(83, 162, 18, 1);
  color: rgba(83, 162, 18, 1);
}

.bottom-box {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  font-size: 22rpx;
  margin-top: 40rpx;
}

.bom-tips {
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
}

.bom-tips image {
  width: 28rpx;
  height: 28rpx;
}

.bom-tips text {
  font-size: 26rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  padding-left: 8rpx;
}
