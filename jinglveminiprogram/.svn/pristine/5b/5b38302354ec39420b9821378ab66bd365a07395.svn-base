/**index.wxss**/
page{
  background: #F7F7F9
}
.head-container{
  background: white;
}
.userinfo,.userinfoshow {
  display: flex;
  align-items: center;
}

.userinfo-avatar {
  width: 140rpx;
  height: 140rpx;
  margin: 20rpx;
  margin-left: 49rpx;
  border-radius: 50%;
  overflow: hidden;
}

.userinfo-nickname {
  width:49rpx;
  height:35rpx;
  font-size:36rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(0,0,0,1);
}
.my-container{
  display: flex;
  justify-content: space-around;
  margin-top: 10rpx;
  padding-bottom: 40rpx;
}
.my-container image{
  width: 66rpx;
  height: 60rpx;
}
.my-container text{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(102,102,102,1);
}
.my-item{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.my-text{
  margin-top: -10rpx;
}

.paddin{
  height: 20rpx;
  width: 100%;
  background-color: rgb(247,247,247);
}

.dialogmess{
  width: 450rpx;
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
  line-height:48rpx;
  text-align: center;
  margin-top: 66rpx;
  margin-left: 98rpx;
}
.gologin-btn{
  width: 199rpx;
  height: 78rpx;
  border-radius: 39rpx;
  background:linear-gradient(0deg,rgba(254,42,42,1),rgba(253,115,41,1));
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height:78rpx;
  text-align: center;
  margin-left: 213rpx;
  margin-top: 38rpx;
  margin-bottom: 44rpx;
}
.beforeremove{
  width: 100%;
  height: 22rpx;
  background: #F7F7F9
}
.removeloagin{
  width: 100%;
  height: 100rpx;
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height: 100rpx;
  text-align: center;
  border-bottom: 1px solid #F7F7F9;
  background: white;
}