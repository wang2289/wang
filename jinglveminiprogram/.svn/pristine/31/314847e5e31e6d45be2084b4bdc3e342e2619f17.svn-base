page{
  background:rgba(247,247,249,1);
}
.container {
  z-index: 1;
  background: #fff;
}

.nav {
  width: 100%;
  overflow-x: auto;
  overflow-y: hidden;
  white-space: nowrap;
  padding: 30rpx 0rpx;
  height: 110rpx;
  box-sizing: border-box;
}

.nav-tab {
  display: inline-block;
  text-align: center;
  line-height: 54rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  margin: 0 36.5rpx;
  position: relative;
}

.nav-tab.active {
  color: #333;
  font-weight: bold;
}

.nav-tab.active::after {
  content: '';
  display: inline-block;
  position: absolute;
  width: 60rpx;
  height: 6rpx;
  background: rgba(254, 63, 54, 1);
  border-radius: 3rpx;
  bottom: -8rpx;
  left: 50%;
  margin-left: -30rpx;
}

.changetab {
  width: 100%;
  height: 79rpx;
  border-top: 1px solid rgba(221, 221, 221, 1);
  border-bottom: 1px solid rgba(221, 221, 221, 1);
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}

.changetit {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  display: flex;
  flex-direction: row;
  align-items: center;
}

.changetit.active {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.awor {
  width: 14rpx;
  height: 12rpx;
  margin-left: 10rpx;
}

.tikulist {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

van-tree-select {
  z-index: 100;
  height: 630rpx;
}

.meng {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  top: 195rpx;
  left: 0rpx;
  z-index: 0;
}

.zongtab {
  width: 100%;
  height: 98rpx;
  line-height: 98rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  background: #fff;
  text-align: center;
  position: relative;
}

.zongtab.active {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.line {
  width: 690rpx;
  height: 1rpx;
  background: rgba(221, 221, 221, 1);
  position: absolute;
  bottom: 0rpx;
  left: 50%;
  margin-left: -345rpx;
}
tiku-cmp{
  border-bottom: 1px solid rgba(221, 221, 221, 1);
}
tiku-cmp:last-child{
  border-bottom: 0px solid rgba(221, 221, 221, 1);
}

