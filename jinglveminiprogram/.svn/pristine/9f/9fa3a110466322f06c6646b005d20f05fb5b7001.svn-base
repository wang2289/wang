.timu-title {
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(0, 0, 0, 1);
  padding: 33rpx 32rpx 22rpx 32rpx;
  display: flex;
  justify-content: space-between;
  z-index: 1;
}
 .nodata{
   text-align: center;
   padding: 30rpx;
 }
.active{
  color: #FE3E2A
}
.timutab {
  /* display: flex; */
  width: 100%;
  overflow: hidden;
  height: 63rpx;
  white-space: nowrap;
  padding: 0 21.5rpx;
  padding-bottom: 25rpx;
    z-index: 1;
}
.timutabs{
  width: 100%;
  height: 100rpx;
  overflow-x: auto;
  padding-right: 22rpx;
}
.timu-tab {
  display: inline-block;
  text-align: center;
  line-height: 54rpx;
  width: 157rpx;
  height: 54rpx;
  border-radius: 27rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  background: rgba(247, 247, 249, 1);
  color: rgba(102, 102, 102, 1);
  margin: 0 10.5rpx;
}

.timu-tab.active {
  background: rgba(255, 247, 247, 1);
  color: rgba(254, 62, 42, 1);
}

