page{
  background-color:#F4F5F7;
  padding-bottom:100rpx;
}
.cart-box{
    /* padding-bottom: 100rpx; */
      background-color:#F4F5F7;
    margin: 20rpx;
 border-radius: 10rpx;
  
    /* margin-bottom:50px; */
}
.cart-list{
  background-color: #fff;
    position: relative;
    padding: 20rpx 20rpx 20rpx 285rpx;
    height: 185rpx;

    width: 100%;
}
.row {
margin-top: 20rpx;
  border-radius:8px;
}
.cart-list .cart-pro-select{
    position: absolute;
    left: 20rpx;
    top: 90rpx;
    width: 45rpx;
    height: 45rpx;
}

.cart-list .cart-thumb{
    position: absolute;
    top: 20rpx;
    left: 85rpx;
    width: 220rpx;
    height: 185rpx;
    border-radius: 8rpx;
   
}
.cart-list .cart-pro-name{
    display: inline-block;
    width: 300rpx;
    height: 105rpx;
    line-height: 50rpx;
    overflow: hidden;
    margin-left: 30rpx;
    font-size:32rpx;
font-weight:500;
color:rgba(51,51,51,1);
}
.cart-list .cart-pro-price{
    display: block;
   font-size:30rpx;
font-weight:400;
color:rgba(254,54,71,1);
    height: 105rpx;
    line-height: 50rpx;  margin-left: 30rpx;
}




.cart-del{
    display: flex;
    

    width:140rpx;
height:225rpx;
background:rgba(254,63,54,1);
border-radius:0px 8px 8px 0px;
justify-content: center;
align-items:  center;
}
.cart-del image{
  width: 34rpx;
  height: 36rpx;

}
.cart-footer{
    position: relative;

    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    padding:0 32rpx 0 80rpx;
    box-sizing: border-box;
    /* margin-bottom: 90rpx; */
    /* background: #AB956D; */
    /* color: #f2f2f2; */
}
.total-select{
    position: absolute;
    left: 20rpx;
    top: 23rpx;
    width: 45rpx;
    height: 45rpx;
    color: #000;
 
}
.order-icon{
    position: absolute;
    right: 40rpx;
    top: 25rpx;
    width: 45rpx;
    height: 45rpx;
}
.order-icon image,.order-icon navigator{
  display: block;
  width: 45rpx;
  height: 45rpx;
}
.cart-toatl-price{
  float: right;
    width: 400rpx;
}

.cart-no-data{
    padding:40rpx 0;
    color: #999;
    text-align: center;
}

.cart-bottom{
  width: 100%;
 height: 98rpx;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  font-size: 24rpx;
 background:rgba(255,255,255,1);

   
}
.datelist{
width: 100%;
height: 100px;

}
.mask {
  display: flex;

  width: 100%;
  height: 100%;
  position: fixed;


  top: 0;
  left: 0;
  background: #F3F3F3;
}
.data-content {
  background-color: #F4F4F4;
  width: 100%;
  height: 100%;
  
  
}
.data-list {
  margin-bottom:40rpx;
  background-color: #fff;
  padding-bottom: 20rpx;
}
.data-title {
height: 100rpx;
line-height: 100rpx;
color:#89898A;
background-color: #F3F3F3;
padding-left: 30rpx;
}
.data-list-type {
  display: flex;
  flex-direction: row;
 justify-content:  space-between;
 padding-left: 30rpx;
 padding-right: 30rpx;
 height: 100rpx;
 line-height: 100rpx;
 border-bottom: 1px solid #F4F3F4;
}
.data-list-select {
   display: flex;
  flex-direction: row;
 justify-content:space-around;
}
label {
  display: block;
  margin-top: 20rpx;
}
.data-foot button{

  background-color: #E93C4F;
  color: #fff;
  width: 95%;
  margin-bottom: 30rpx;
}
.cart-jiesuan {
  float: right;
  width:199rpx;
height:78rpx;
background:linear-gradient(0deg,rgba(254,42,42,1),rgba(253,115,41,1));
border-radius:39rpx;
text-align: center;
color: #fff;
line-height: 78rpx;
font-size:28rpx;
}
.money {
  font-weight:400;
color:rgba(254,54,71,1);
font-size:30rpx;
}
.cart-num{
    position: absolute;
  right: 350rpx;
    bottom: 24rpx;
    width: 80rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
  font-size:24rpx;
font-weight:400;
color:rgba(153,153,153,1);
}
