.container {
  width: 750rpx;
  justify-content: flex-start;
}

.chapter {
  width: 100%;
  display: flex;
  flex-direction: column;
}

.section {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 38rpx 0rpx 30rpx 0rpx;
}

.section image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 12rpx;
  margin-left: 30rpx;
}

image.icon {
  width: 32rpx;
  height: 32rpx;
  margin-right: 12rpx;
  margin-left: 30rpx;
}

.title {
  padding: 38rpx 60rpx 30rpx 76rpx;
  box-sizing: border-box;
  height: auto;
  background: rgba(247, 247, 249, 1);
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  position: relative;
}
.section2{
  position: relative;
}
.title .line {
  width: 690rpx;
  height: 1px;
  background: rgba(221, 221, 221, 1);
  position: absolute;
  bottom: 1px;
  right: 0rpx;
}

.none {
  display: none;
}

.van-collapse-item__content {
  padding: 0px !important;
  font-size: 13px;
  line-height: 1.5;
  color: #999;
  background-color: #fff;
}

.van-cell__title {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.tag {
  width: 77rpx;
  height: 31rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 16rpx;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  text-align: center;
  position: absolute;
  right: 24rpx;
  top: 50%;
  margin-top: -15.5rpx;
}

.lock {
  width: 23rpx;
  height: 17rpx;
  position: absolute;
  right: 24rpx;
  top: 50%;
  margin-top: -8.5rpx;
}
