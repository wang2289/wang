<view class="container">
  <view class="timu-title">
    <text>{{title}}</text>
    <van-icon bindtap='onCollect' custom-class="{{isCollect?'active':''}}" name="{{isCollect?'star':'star-o'}}" />
  </view>
  <view class="timutab">
    <view class="timutabs">
      <block wx:for="{{timutab}}" wx:key="*this">
        <text class="timu-tab {{isActive == index+1 ? 'active':'' }}" data-tab="{{index+1}}" data-id='{{item.id}}' bind:tap="onActive">{{item.title}}</text>
      </block>
    </view>
  </view>
  <scroll-view scroll-y="true" style="height:1060rpx">
    <view class="course-box">
      <course-cmp wx:if="{{courses}}" ismark="次" type="1" isios="{{isIos}}" bind:addInfo="getAddInfo" title='{{title}}' money='{{money}}' course="{{tilist}}"></course-cmp>
      <courses-cmp wx:if="{{!courses}}" ismark="次" type="1" isios="{{isIos}}" bind:addInfo="getAddInfo" title='{{title}}' money='{{money}}' course="{{tilist}}"></courses-cmp>
      <mp-loading type="circle" show="{{showl}}"></mp-loading>
      <view class="nodata" wx:if="{{nodata}}">暂无数据</view>
    </view>
  </scroll-view>
</view>
<mengl-cmp title="{{tihsi}}" money='{{money}}' isios="{{isIos}}" isshow='{{showif}}' aomid='{{aomid}}' />
<meng-cmp title="{{tihsi1}}"  isshow='{{showifs1}}' />
<van-dialog catchtouchmove="true" show-confirm-button="{{false}}" customStyle="width: 640rpx;height: 408rpx; " use-slot title="提示" show="{{ show }}" close-on-click-overlay="{{false}}" confirm-button-text="去绑定">
  <view class="dialogmess">
    <text>该微信还未注册精略网校或未绑定精略网校账号。</text>
  </view>
  <view class="gologin-btn" bindtap="gotologin">
    <text>去绑定</text>
  </view>
</van-dialog>
<van-toast id="van-toast" />