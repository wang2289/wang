
.container {
  width: 690rpx;
  height: auto;
  display: flex;
  flex-direction: row;
  padding: 32rpx 0rpx;
}

.tiku-img image {
  width: 268rpx;
  height: 150rpx;
  border-radius: 8rpx;
  margin-right: 15rpx;
}

.tiku-detail {
  display: flex;
  flex-direction: column;
  position: relative;
  width: 100%;
}

.tiku-title {
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.tiku-bottom {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  position: absolute;
  bottom: 0rpx;
  width: 100%;
}

.see {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.seeicon {
  width: 28rpx;
  height: 22rpx;
  margin-right: 8rpx;
}

.money {
  font-size: 34rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(254, 54, 71, 1);
}

.isbuy {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}
