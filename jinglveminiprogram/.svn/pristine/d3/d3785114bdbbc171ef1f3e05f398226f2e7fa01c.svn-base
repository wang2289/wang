<view class='container'>
  <view class="tiku-img">
    <image class="" src="{{tiku.imgurl}}"></image>
  </view>
  <view class="tiku-detail">
    <view class="tiku-title">{{tiku.title}}</view>
    <view class="tiku-bottom">
        <view class="see"><image class="seeicon" src="/images/icon_bofangliang@2x.png"></image>{{tiku.see}}</view>
        <view class="money" wx-if="{{tiku.isbuy}}">￥{{tiku.money}}</view>
        <view class="isbuy" wx-if="{{!tiku.isbuy}}">已购买</view>
    </view>
  </view>
</view>