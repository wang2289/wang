
page{
  background-color: #F7F7F9;
}
.title {
font-size:24rpx;

font-weight:400;
color:rgba(102,102,102,1);
height: 80rpx;
line-height: 80rpx;
background-color: #F7F7F9;
padding-left: 30rpx;
}
.address {
  position: relative;
  height: 141rpx;
display: flex;
justify-items:  center;
 align-items: center;
    margin: 20rpx;
 border-radius: 10rpx;
    background-color: #fff;
   
}
.divider{
   border-bottom: 1rpx solid #E5E5E5;
}
.addressicon{
  width: 72rpx;
  height: 72rpx;
  padding-left: 20rpx;
  padding-right: 15rpx;
}
.addressmor {
  position: absolute;

  right: 30rpx;
  width: 14rpx;
  height: 24rpx;
}
.address-detail {
font-weight:500;
color:rgba(51,51,51,1);
font-size:30rpx;
}
.conact text {
font-size:26rpx;
font-weight:400;
color:rgba(51,51,51,1);
}
.conact span{
font-size:26rpx;
font-weight:400;
color:rgba(153,153,153,1);
margin-left: 10rpx;
}
.delete-addres{
  width: 127rpx;
  height:  141rpx;
  background: #FE3F36;
  display: flex;
  align-items: center;
  justify-content: center;
}
.foot{
  position: fixed;
  width: 100%;
  border-top: 1px solid #E1E1E2;
  bottom: 0px;
 display: flex;
 align-content: center; 
 align-items: center;
 justify-content: center;
 font-weight:400;
color:rgba(51,51,51,1);
height:98rpx;
background:rgba(255,255,255,1);
font-size:30rpx;
}
.foot image{
width: 35rpx;
height: 35rpx;
padding-right: 10rpx;
}
