.m-ad-item {
  background-color: #fff;
  padding: 24rpx 20rpx 24rpx 30rpx;
  position: relative;
  border-bottom: 1rpx solid #d5d5d5;
}

.m-ad-l {
  width: 100%;
  display: inline-block;
}

.m-ad-i-top {
  font-size: 32rpx;
  line-height: 40rpx;
}

.m-ad-i-top text {
  margin-right: 20rpx;
}

.m-ad-i-bot {
  font-size: 28rpx;
  color: #888;
  line-height: 40rpx;
  margin-top: 10rpx;
}

.m-ad-icon {
  font-size: 24rpx;
  padding: 4rpx 8rpx;
  background-color: #db384c;
  color: #fff;
  border-radius: 8rpx;
  margin-right: 8rpx;
  text-align: center;
  display: initial;
}

.m-ad-edit {
  position: absolute;
  right: 20rpx;
  top: 50%;
  width: 80rpx;
  height: 100rpx;
  margin-top: -50rpx;
  padding-left: 40rpx;
}

.m-ad-edit label {
  width: 40rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 35rpx;
  display: block;
  vertical-align: middle;
  text-align: center;
  color: #888;
}

