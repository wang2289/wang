}

.m-total-btn {
  text-align: right;
  padding: 20rpx 20rpx 20rpx 20rpx;
  position: relative;
}

.m-total-btn::before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid #d9d9d9;
  color: #d9d9d9;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.m-total-btn .u-link-btn {
  margin: 0 10rpx;
  vertical-align: middle;
  display: inline-block;
  line-height: 52rpx;
  background-color: #fff;
}
.i-link-btn{
  border: 1rpx solid #ea281a;
  color: #ea281a;
}
.m-navbar-items {
  position: relative;
  display: block;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  padding: 26rpx 0;
  text-align: center;
  font-size: 28rpx;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  z-index: 2;
}
.m-total-price{
  position:relative;
top:0rpx;
  font-weight: bold;
}
/*.m-navbar-item:active {background-color: #ededed;}*/

/* .m-navbar-items.m-navbar-item-on {background-color: #eaeaea;} */

.m-navbar-items:after {
  content: " ";
  position: absolute;
  right: 0;
  top: 0;
  width: 1px;
  bottom: 0;
  color: #ccc;
  -webkit-transform-origin: 100% 0;
  transform-origin: 100% 0;
  -webkit-transform: scaleX(0.5);
  transform: scaleX(0.5);
}

.m-navbar-items:last-child:after {
  display: none;
}

