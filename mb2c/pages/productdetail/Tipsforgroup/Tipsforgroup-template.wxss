.Tipsfor-container {
  display: flex;
  flex-direction: column;
  height: 160rpx;
  background: #fff;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.tips-title {
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  border-bottom: 1px solid #eee;
}
.rightarrow{
  width: 14rpx;
  height: 24rpx;
  margin-right: 0rpx;
  vertical-align: middle
}
.tips-container {
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.tips-title .tips-left {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #333;
  margin-left: 30rpx;
}

.tips-title .tips-right {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #999;
  margin-right: 20rpx;
}

.tips-container .tips-left {
  font-size: 28rpx;
  color: #666;
}

.tips-container .tips-right {
  font-size: 28rpx;
  color: #666;
}
