.m-navbar-items.m-navbar-item-on {background-color: #fff;color: #DB384C;}
.m-navbar-items.m-navbar-item-on::before {content: " ";position: absolute;left: 0;bottom: 0;right: 0;height: 6rpx;border-bottom: 6rpx solid #DB384C;color: #CCCCCC;-webkit-transform-origin: 0 100%;transform-origin: 0 100%;-webkit-transform: scaleY(0.5);transform: scaleY(0.5);z-index: 3;}
.m-navbar-items {position: relative;display: block;-webkit-box-flex: 1;-webkit-flex: 1;flex: 1;padding: 13rpx 0;text-align: center;font-size: 15px;-webkit-tap-highlight-color: rgba(0, 0, 0, 0);z-index: 2;}
/*.m-navbar-item:active {background-color: #ededed;}*/
/* .m-navbar-items.m-navbar-item-on {background-color: #eaeaea;} */
.m-navbar-items:after {content: " ";position: absolute;right: 0;top: 0;width: 1px;bottom: 0;color: #CCCCCC;-webkit-transform-origin: 100% 0;transform-origin: 100% 0;-webkit-transform: scaleX(0.5);transform: scaleX(0.5);}
.m-navbar-items:last-child:after {display: none;}
