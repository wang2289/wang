/*loading start*/
.u-loadmore {width: 65%;margin: 1.5em auto;line-height: 1.6em;font-size: 28rpx;text-align: center;}
.u-loadmore-tips {display: inline-block;vertical-align: middle;}
.u-loadmore-line {border-top: 2rpx solid #E5E5E5;margin-top: -6.6em;}
.u-loadmore-line .u-loadmore-tips {position: relative;top: -0.9em;padding: 0 .55em;background-color: #f8f8f8;color: #999999;}
.u-loadmore-dot .u-loadmore-tips {padding: 0 .16em;}
.u-loadmore-dot .u-loadmore-tips:before {content: " ";width: 8rpx;height: 8rpx;border-radius: 50%;background-color: #E5E5E5;display: inline-block;position: relative;vertical-align: 0;top: -0.16em;}
.u-loading {width: 40rpx;height: 40rpx;display: inline-block;vertical-align: middle;-webkit-animation: weuiLoading 1s steps(12, end) infinite;animation: weuiLoading 1s steps(12, end) infinite;background: transparent url(data:image/svg+xml;base64,) no-repeat;background-size: 100%;}
@-webkit-keyframes weuiLoading {
  0% {
    -webkit-transform: rotate3d(0, 0, 1, 0deg);
            transform: rotate3d(0, 0, 1, 0deg);
  }
  100% {
    -webkit-transform: rotate3d(0, 0, 1, 360deg);
            transform: rotate3d(0, 0, 1, 360deg);
  }
}
@keyframes weuiLoading {
  0% {
    -webkit-transform: rotate3d(0, 0, 1, 0deg);
            transform: rotate3d(0, 0, 1, 0deg);
  }
  100% {
    -webkit-transform: rotate3d(0, 0, 1, 360deg);
            transform: rotate3d(0, 0, 1, 360deg);
  }
}